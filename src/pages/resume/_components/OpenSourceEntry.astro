---
import Entry from "./BaseEntry.astro";
import DateRange from "./DateRange.astro";
import type { CvEntry } from "@/content";

type Props = { entry: CvEntry<"open-source"> };
const { entry } = Astro.props;
---

<Entry entry={entry}>
  <a
    slot="heading"
    href={entry.data.githubUrl}
    target="_blank"
  >
    <span>{entry.data.title}</span>
    <div class="i-ph-arrow-up-right-bold inline-block"></div>
  </a>
  <Fragment slot="subheading">{entry.data.description}</Fragment>
</Entry>
