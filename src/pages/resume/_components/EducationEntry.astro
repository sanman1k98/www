---
import Entry from "./BaseEntry.astro";
import DateRange from "./DateRange.astro";
import type { CvEntry } from "@/content";

type Props = { entry: CvEntry<"education"> };
const { entry } = Astro.props;
---

<Entry>
  <Fragment slot="heading">{entry.data.major}</Fragment>
  <Fragment slot="subheading">{entry.data.school.name}</Fragment>
  <DateRange slot="after-subheading" { entry }/>
</Entry>
