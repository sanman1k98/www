---
import type { CollectionEntry } from "astro:content";

type Props = { entry?: CollectionEntry<"cv"> };
const { entry } = Astro.props;
let Content;

if (entry) {
  const rendered = await entry.render();
  Content = rendered.Content;
}
---

<article class="m-3">
  <header>
    <slot />
  </header>
  {Content && <Content />}
</article>
