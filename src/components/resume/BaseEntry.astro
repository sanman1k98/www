---
import type { CollectionEntry } from "astro:content";

type Props = { entry: CollectionEntry<"cv"> };

const { entry } = Astro.props;

const { Content } = await entry.render();
---

<article>
  <header>
    <slot name="heading" />
    <slot name="subheading" />
    <slot name="time" />
  </header>
  <Content />
</article>
