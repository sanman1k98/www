{
  "type": "module",
  "private": "true",
  "scripts": {
    "dev": "bun --bun run astro dev --host",
    "start": "bun --bun run astro dev --host",
    "check": "bun --bun run astro check && tsc --noEmit",
    "build": "bun --bun run astro build",
    "preview": "bun --bun run astro preview --host",
    "deploy": "./scripts/deploy.sh && git push origin deploy",
    "dev:personal": "BUILD='personal' bun --bun run astro dev --host",
    "build:personal": "BUILD='personal' bun --bun run astro build",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/sitemap": "3.1.6",
    "@fontsource-variable/quicksand": "^5.0.18",
    "@fontsource-variable/recursive": "^5.0.18",
    "@fontsource-variable/shantell-sans": "^5.0.22",
    "@fontsource/quicksand": "^5.0.18",
    "@iconify-json/carbon": "^1.1.35",
    "@iconify-json/ph": "^1.1.13",
    "@resvg/resvg-js": "^2.6.2",
    "astro": "4.11.0",
    "astro-icon": "^1.1.0",
    "culori": "^4.0.1",
    "exif-reader": "^2.0.1",
    "github-slugger": "^2.0.0",
    "react": "^18.3.1",
    "satori": "^0.10.13",
    "sharp": "^0.33.4",
    "unocss": "^0.58.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@astrojs/check": "0.7.0",
    "@astrojs/ts-plugin": "^1.8.0",
    "@octokit/openapi-types": "^22.2.0",
    "@types/bun": "^1.1.3",
    "@types/culori": "^2.1.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^7.13.1",
    "@unocss/core": "^0.58.9",
    "@unocss/eslint-plugin": "^0.58.9",
    "@unocss/preset-mini": "^0.58.9",
    "@unocss/reset": "^0.58.9",
    "eslint": "^9.5.0",
    "eslint-plugin-astro": "^1.2.2",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "prettier": {
    "plugins": ["prettier-plugin-astro"],
    "editorconfig": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro",
          "astroAllowShorthand": true,
          "astroSkipFrontmatter": true
        }
      }
    ]
  }
}
